{
  "name": "jay-freestone-portfolio",
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "routes": [
    {
      "src": "/journal/(?<slug>[^/]*)",
      "dest": "/writing/$id",
      "status": 301
    }
  ],
  "env": {
    "SITE_URL": "https://www.jayfreestone.com",
    "ANALYTICS_UID": "UA-133309119-1"
  }
}